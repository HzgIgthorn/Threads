package de.udemy.firstthread;

public class firstWay {

	public static void main(String[] args) {
		new FirstTask();
	}
}

class FirstTask extends Thread{
	
	public FirstTask() {
		this.start();
	}

	@Override
	public void run() {
		for(int i = 10; i > 0; i-- ) {
			System.out.println("TICK TICK " + i);
		}
	}
}
